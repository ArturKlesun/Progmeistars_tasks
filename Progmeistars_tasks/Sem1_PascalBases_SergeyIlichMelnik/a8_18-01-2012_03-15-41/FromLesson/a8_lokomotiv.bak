Program a9_{Заставить введённое слово заставить паровозиком бегать по квадрату}Vilciens;
Uses CRT;

Type Wheray = Array [1..255] of LongInt;
Type Whoray = Array [1..255] of char;
Var x,y: Dray;
    vag: Whoray;

Procedure RazobratjNaBukvi(s: string);
var i1: integer;
Begin
  For i1:=1 to length(s) do begin;
   vag[i1]:=(s, i1, 1);
   writeln[i1];
  End;
End;

Procedure NextCell; {x,y - клеточка. Вычисляет следующую клеточку}
Var wx, wy, i1, i2: integer;
Begin
  Repeat
   wx:=1;
   While x[wx]< WindMaxX do begin {x[i]=последняя буква}



    wx:=wx+1;
   End;
  until KeyPressed
End;

Var wx, wy, i1, i2: integer;
    lokomotiv: string;

Begin
  Writeln;
  Writeln('Здравствуйте, я контроллёр Валдис, отвечаю за логичное движение вагонов по рельсам.');
  Writeln('Пожалуйста, введите слово, по буквам которого будут названы вагоны:');
  Readln(lokomotiv);
  RazobratjNaBukvi(lokomotiv);
End.